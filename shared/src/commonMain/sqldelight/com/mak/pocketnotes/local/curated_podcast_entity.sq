CREATE TABLE CuratedPodcastEntity (
    id TEXT PRIMARY KEY NOT NULL,
    title TEXT NOT NULL,
    image TEXT,
    thumbnail TEXT,
    sectionId TEXT NOT NULL,
    FOREIGN KEY(sectionId) REFERENCES CuratedSectionEntity(id) ON DELETE CASCADE
);

insertCuratedPodcast:
